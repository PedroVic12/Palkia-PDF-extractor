<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<NotepadPlus>
    <AutoComplete language="Organon_Script_Plugin">
    <Environment ignoreCase="yes" startFunc="(" stopFunc=")" paramSeparator="," terminal=";" additionalWordChar="." />
        
        <KeyWord name="DEMT" func="yes">
            <Overload retVal="" descr="Dados de Barramentos Candidatos">
                <Param name="(BCan (   Iden   ) (SE) (SA) (Ar) (P.CC) (Pmx ) (P)" />
            </Overload>
        </KeyWord>
        <KeyWord name="DGMT" func="yes">
            <Overload retVal="" descr="Dados de Gerações das Subestações">
                <Param name="(SE) (Bger (Pmx ) (P) (Qmn) (Qmx)" />
            </Overload>
        </KeyWord>
        <KeyWord name="DWMT" func="yes">
            <Overload retVal="" descr="Dados de SWING P/ MARGEM">
                <Param name="#SE  Tp(1=System/2=Area/2=Zone/4=Bus)   0/#Area/#Zone/#Bus" />
            </Overload>
        </KeyWord>

        <KeyWord name="BUS" func="yes">
            <Overload retVal="" descr="Altera dados de barra">
                <Param name="#barra NAME=V ANG=V GSHT=V BSHT=V SHTST=V" />
            </Overload>
        </KeyWord>
        <KeyWord name="GEN" func="yes">
            <Overload retVal="" descr="Altera dados de geração">
                <Param name="#barra V=V PG=V PMAX=V QMAX=V QMIN=V" />
            </Overload>
        </KeyWord>

